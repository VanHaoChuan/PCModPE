<Window x:Class="PCModPE.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:cefSharp="clr-namespace:CefSharp.Wpf;assembly=CefSharp.Wpf"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:scintillaNet="clr-namespace:ScintillaNET;assembly=ScintillaNET"
        xmlns:local="clr-namespace:PCModPE"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TabControl>
            <TabItem Header="测试页面">
                <cefSharp:ChromiumWebBrowser Name="myWebView" ></cefSharp:ChromiumWebBrowser>
            </TabItem>
            <TabItem Header="代码编辑页面">
            </TabItem>
            <TabItem Header="文档">
                <cefSharp:ChromiumWebBrowser Name="documenthtml"></cefSharp:ChromiumWebBrowser>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
